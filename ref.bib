@article{Abourachid2001,
abstract = {The importance of size, functional features and morphological features in adaptation for walking in birds were studied. The time and space kinematic parameters of locomotion were compared in two running birds, the ratites (rhea, kiwi, Paleognatiforms), in two swimming birds, (ducks) and two striding birds, (quail and Guinea fowl). The results showed that in the two phases, stance and swing, the time and space parameters worked in opposite ways: the duration of the swing was constant, but its length increased with speed. In contrast, the duration of the stance was correlated to speed, while its length was not (except in ducks). In all the birds, a higher speed was achieved by a decrease of the stance duration, and an increase of the swing length. The kinematic parameters were not used in the same way in all species: There is a size effect and large birds increase their speed mainly by increasing the frequency of their movements and the small species increase mainly their amplitude. Nevertheless, it is not the main factor and morphology, such as swimming adaptation features of the ducks, and behaviour, are important because they modify the mechanical constraints and influence the kinematics parameters. {\textcopyright} 2001 Elsevier Science Inc. All rights reserved.},
author = {Abourachid, Anick},
doi = {10.1016/S1095-6433(01)00471-8},
file = {:home/kennethchao/Documents/Mendeley Desktop/Abourachid - 2001 - Kinematic parameters of terrestrial locomotion in cursorial (ratites), swimming (ducks), and striding birds (quail a.pdf:pdf;::},
isbn = {1095-6433},
issn = {10956433},
journal = {Comparative Biochemistry and Physiology Part A: Molecular and Integrative Physiology},
keywords = {Anatidae,Aves,Bipedal locomotion,Functional morphology,Galliforms,Kinematics,Ratites},
mendeley-groups = {FastRunner,FastRunner/2018{\_}ARO{\_}Proposal,FastRunner/2016{\_}FastRunnerProposal},
month = {dec},
number = {1},
pages = {113--119},
pmid = {11733170},
title = {{Kinematic parameters of terrestrial locomotion in cursorial (ratites), swimming (ducks), and striding birds (quail and guinea fowl)}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11733170},
volume = {131},
year = {2001}
}
@article{Abourachid2011,
abstract = {Most birds use at least two modes of locomotion: flying and walking (terrestrial locomotion). Whereas the wings and tail are used for flying, the legs are mainly used for walking. The role of other body segments remains, however, poorly understood. In this study, we examine the kinematics of the head, the trunk, and the legs during terrestrial locomotion in the quail (Coturnix coturnix). Despite the trunk representing about 70{\%} of the total body mass, its function in locomotion has received little scientific interest to date. This prompted us to focus on its role in terrestrial locomotion. We used high-speed video fluoroscopic recordings of quails walking at voluntary speeds on a trackway. Dorso-ventral and lateral views of the motion of the skeletal elements were recorded successively and reconstructed in three dimensions using a novel method based on the temporal synchronisation of both views. An analysis of the trajectories of the body parts and their coordination showed that the trunk plays an important role during walking. Moreover, two sub-systems participate in the gait kinematics: (i) the integrated 3D motion of the trunk and thighs allows for the adjustment of the path of the centre of mass; (ii) the motion of distal limbs transforms the alternating forward motion of the feet into a continuous forward motion at the knee and thus assures propulsion. Finally, head bobbing appears qualitatively synchronised to the movements of the trunk. An important role for the thigh muscles in generating the 3D motion of the trunk is suggested by an analysis of the pelvic anatomy. {\textcopyright} 2011 Elsevier GmbH.},
author = {Abourachid, Anick and Hackert, Remi and Herbin, Marc and Libourel, Paul A. and Lambert, Fran{\c{c}}ois and Gioanni, Henri and Provini, Pauline and Blazevic, Pierre and Hugel, Vincent},
doi = {10.1016/j.zool.2011.07.002},
file = {:home/kennethchao/Documents/Mendeley Desktop/Abourachid et al. - 2011 - Bird terrestrial locomotion as revealed by 3D kinematics.pdf:pdf},
isbn = {1873-2720 (Electronic)$\backslash$n0944-2006 (Linking)},
issn = {09442006},
journal = {Zoology},
keywords = {3D kinematics,Bird anatomy,Bird locomotion,Coturnix coturnix,Head bobbing,fastrunner},
mendeley-groups = {FastRunner,FastRunner/2018{\_}ARO{\_}Proposal,FastRunner/2016{\_}FastRunnerProposal},
mendeley-tags = {fastrunner},
month = {dec},
number = {6},
pages = {360--368},
pmid = {21982408},
title = {{Bird terrestrial locomotion as revealed by 3D kinematics}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21982408},
volume = {114},
year = {2011}
}
