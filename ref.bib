mendeley-tags = {fastrunner},
number = {5},
pages = {251--256},
pmid = {15352876},
title = {{Histochemical and morphometrical characterization and distribution of fibre types in four muscles of ostrich (Struthio camelus)}},
url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1439-0264.2004.00535.x/full},
